{
  "update.mode": "default",

  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "compact",
  "window.density.editorTabHeight": "default",
  "window.newWindowDimensions": "inherit",
  "window.zoomLevel": 0,
  "window.commandCenter": true,

  "files.eol": "\n",

  "debug.console.fontFamily": "JetBrains Mono",
  "debug.console.fontSize": 14,
  "debug.console.lineHeight": 0,

  "breadcrumbs.enabled": false,
  "breadcrumbs.symbolPath": "off",

  "zenMode.hideLineNumbers": false,
  "search.showLineNumbers": true,

  "editor.tabSize": 2,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontSize": 14,
  "editor.lineHeight": 24,
  "editor.fontWeight": "normal",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.linkedEditing": true,
  "editor.renderLineHighlight": "all",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "always",
    "source.fixAll.editorconfig": "always"
  },
  "editor.pasteAs.preferences": ["chat.attach.attachments"],
  "editor.smoothScrolling": true,
  "editor.suggestLineHeight": 35,
  "editor.semanticHighlighting.enabled": false,
  "editor.inlayHints.fontSize": 12,
  "editor.renderWhitespace": "none",
  "editor.inlayHints.padding": true,
  "editor.suggestSelection": "first",
  "editor.inlineSuggest.enabled": true,
  "editor.stickyScroll.enabled": false,
  "editor.suggest.showSnippets": false,
  "editor.parameterHints.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.guides.bracketPairs": "active",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.snippetSuggestions": "bottom",
  "editor.suggest.preview": true,
  "editor.hover.delay": 500,
  "editor.trimAutoWhitespace": false,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.colorDecoratorsActivatedOn": "click",
  "editor.wordSeparators": "`~!@#$%^&*-()=+[{]}\\|;:'\",.<>/?",
  "editor.fontVariations": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero'",
  "editor.fontLigatures": "'ss01','ss02','ss03', 'cv01','cv02','cv10','cv03','cv06','cv09'",
  "editor.guides.indentation": true,
  "editor.overviewRulerBorder": false,
  "editor.cursorBlinking": "phase",
  "chat.agent.maxRequests": 100,
  "chat.mcp.gallery.enabled": true,
  "chat.agentSessionsViewLocation": "single-view",
  "chat.showAgentSessionsViewDescription": true,
  "chat.agent.thinkingStyle": "collapsedPreview",
  "chat.editing.confirmEditRequestRemoval": false,
  "chat.commandCenter.enabled": false,
  "chat.edits2.enabled": true,
  "chat.emptyState.history.enabled": true,
  "chat.notifyWindowOnResponseReceived": false,
  "chat.todoListTool.descriptionField": true,
  "inlineChat.enableV2": true,
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.chat.generateTests.codeLens": true,
  "github.copilot.chat.edits.suggestRelatedFilesFromGitHistory": false,
  "github.copilot.chat.edits.suggestRelatedFilesForTests": false,
  "github.copilot.chat.newWorkspaceCreation.enabled": false,
  "github.copilot.chat.useProjectTemplates": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "scminput": false
  },
  "chat.tools.terminal.autoApprove": {
    "mv": true,
    "npm install": true,
    "npx": true,
    "npm run format": true,
    "mvn spring-boot:run": true,
    "git rev-parse": true,
    "git add": true,
    "git commit": true,
    "npm run build": true,
    "npm run dev": true,
    "docker-compose": true,
    "ngrok": true,
    "docker ps": true,
    "docker logs": true,
    "temporal": true,
    "mkdir": true,
    "cp": true,
    "go": true,
    "ssh": true,
    "lsof": true,
    "npm run start": true,
    "git fetch": true,
    "git restore": true,
    "md5sum": true,
    "scp": true
  },

  "workbench.colorTheme": "Min Dark",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.editor.empty.hint": "hidden",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.iconTheme": "symbols",
  "workbench.startupEditor": "readme",
  "workbench.editor.enablePreview": false,
  "workbench.list.smoothScrolling": true,
  "workbench.layoutControl.enabled": false,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 14,
  "workbench.colorCustomizations": {
    "scrollbar.shadow": "#00000000",

    "editor.overviewRulerBorder": false,

    "editor.lineHighlightBorder": "#00000000",
    "editorBracketMatch.background": "#43454a",
    "editorBracketMatch.border": "#00000000",
    "editorGutter.background": "#00000000",

    "gitDecoration.modifiedResourceForeground": "#74aeec",
    "gitDecoration.stageModifiedResourceForeground": "#74aeec",
    "gitDecoration.addedResourceForeground": "#7fd27fd9",
    "gitDecoration.renamedResourceForeground": "#74aeec",
    "gitDecoration.untrackedResourceForeground": "#f7aeae",
    "gitDecoration.ignoredResourceForeground": "#ffffff3b"
  },

  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-lock*, package-lock*, bun.lockb",
    "tailwind.config*": "tailwind.config*, postcss.config*"
  },

  "files.autoSaveWorkspaceFilesOnly": true,
  "files.associations": { "*.jsx": "javascriptreact" },

  "prettier.semi": true,
  "prettier.printWidth": 80,
  "prettier.singleQuote": false,
  "prettier.useTabs": true,
  "prettier.useEditorConfig": true,

  "eslint.codeActionsOnSave.mode": "all",
  "eslint.format.enable": true,
  "eslint.useESLintClass": true,
  "eslint.useFlatConfig": true,
  "eslint.probe": ["javascript", "typescript", "svelte", "html", "json", "jsonc", "yaml", "github-actions-workflow"],
  "eslint.validate": ["javascript", "typescript", "svelte", "html", "json", "jsonc", "yaml", "github-actions-workflow"],

  "terminal.integrated.fontFamily": "JetBrains Mono Nerd Font",
  "terminal.integrated.lineHeight": 1.4,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.tabs.location": "right",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.env.windows": {},
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.fontLigatures.fallbackLigatures": [
    "<--",
    "<---",
    "<<-",
    "<-",
    "->",
    "->>",
    "-->",
    "--->",
    "<==",
    "<===",
    "<<=",
    "<=",
    "=>",
    "=>>",
    "==>",
    "===>",
    ">=",
    ">>=",
    "<->",
    "<-->",
    "<--->",
    "<---->",
    "<=>",
    "<==>",
    "<===>",
    "<====>",
    "::",
    ":::",
    "<~~",
    "</",
    "</>",
    "/>",
    "~~>",
    "==",
    "!=",
    "/=",
    "~=",
    "<>",
    "===",
    "!==",
    "!===",
    "<:",
    ":=",
    "*=",
    "*+",
    "<*",
    "<*>",
    "*>",
    "<|",
    "<|>",
    "|>",
    "+*",
    "=*",
    "=:",
    ":>",
    "/*",
    "*/",
    "+++",
    "<!--",
    "<!---"
  ],

  "emmet.triggerExpansionOnTab": true,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "postcss": "css",
    "razor": "html"
  },

  "[markdown]": {
    "editor.wordWrapColumn": 50,
    "diffEditor.ignoreTrimWhitespace": false
  },
  "[css]": { "editor.suggest.showVariables": true },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.snippetSuggestions": "bottom"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java"
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[jsonc]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false
  },

  "security.workspace.trust.enabled": false,

  "http.systemCertificates": false,

  "git.mergeEditor": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.openRepositoryInParentFolders": "never",
  "git.ignoreRebaseWarning": true,

  "gitlens.views.showRelativeDateMarkers": false,

  "typescript.tsserver.log": "off",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.format.enable": false,
  "typescript.tsserver.enableTracing": true,
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.tsserver.nodePath": "node",
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "typescript.preferences.preferTypeOnlyAutoImports": true,

  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.format.enable": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",

  "python.analysis.typeCheckingMode": "basic",

  "diffEditor.renderSideBySide": true,
  "diffEditor.ignoreTrimWhitespace": false,

  "redhat.telemetry.enabled": true,

  "gitlens.views.scm.grouped.views": {
    "commits": false,
    "branches": true,
    "remotes": true,
    "stashes": false,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "repositories": false,
    "searchAndCompare": false,
    "launchpad": false
  },

  "docker.extension.enableComposeLanguageServer": true,
  "docker.extension.dockerEngineAvailabilityPrompt": false,

  "makefile.configureOnOpen": true,

  "githubIssues.ignoreCompletionTrigger": [
    "coffeescript",
    "crystal",
    "diff",
    "dockerfile",
    "dockercompose",
    "ignore",
    "ini",
    "julia",
    "makefile",
    "perl",
    "powershell",
    "python",
    "r",
    "ruby",
    "shellscript",
    "yaml",
    "yml"
  ]
}
